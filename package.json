{
	"name": "fireworks",
	"displayName": "fireworks",
	"description": "A vibrant and dynamic dark theme for VS Code",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.74.0"
	},
	"categories": [
		"Themes"
	],
	"contributes": {
		"themes": [
			{
				"label": "Fireworks",
				"uiTheme": "vs-dark",
				"path": "./themes/Fireworks-color-theme.json"
			}
		]
	},
	"keywords": [
		"dark",
		"light",
		"simplict",
		"theme"
	],
	"devDependencies": {
		"chroma-js": "^2.4.2",
		"nodemon": "^2.0.20"
	},
	"scripts": {
		"start": "nodemon --watch src src/index.js",
		"build": "node src/index.js"
	}
}
